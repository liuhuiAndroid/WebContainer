    <application
        android:allowBackup="true"
        android:icon="@mipmap/hnicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.rupeng119.huainan.MainActivity">

        </activity>

        <activity android:name="com.rupeng119.huainan.PictureActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>